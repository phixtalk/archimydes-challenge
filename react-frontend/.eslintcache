ge={(e) => setRole(e.target.value)}\r\n              defaultValue={role}\r\n            >\r\n              <option>Select a Role</option>\r\n              <option>User</option>\r\n              <option>Admin</option>\r\n            </Form.Control>\r\n          </Form.Group>\r\n        </Form>\r\n      </Modal.Body>\r\n\r\n      <Modal.Footer>\r\n        {/* {closeModal && handleClose()} */}\r\n        {userLoading && (\r\n          <span\r\n            className=\"text-center\"\r\n            style={{\r\n              color: \"#000000\",\r\n              width: \"100%\",\r\n              display: \"block\",\r\n            }}\r\n          >\r\n            Processing...\r\n          </span>\r\n        )}\r\n        <Button\r\n          variant=\"default\"\r\n          onClick={handleClose}\r\n          style={{\r\n            color: \"#000000\",\r\n            width: \"100%\",\r\n            fontWeight: \"bold\",\r\n            display: \"block\",\r\n          }}\r\n        >\r\n          Cancel\r\n        </Button>\r\n        <Button\r\n          variant=\"default\"\r\n          onClick={handleSubmit}\r\n          style={{\r\n            backgroundColor: \"#00b8c5\",\r\n            color: \"#ffffff\",\r\n            width: \"100%\",\r\n            display: \"block\",\r\n          }}\r\n        >\r\n          {modalData?.action === \"create\" ? \"Create\" : \"Update\"} User\r\n        </Button>\r\n      </Modal.Footer>\r\n    </Modal>\r\n  );\r\n};\r\n\r\nexport default UserInfo;\r\n",{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"111","replacedBy":"115"},{"ruleId":"113","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":15,"column":14,"nodeType":"119","messageId":"120","endLine":15,"endColumn":23},"no-native-reassign",["121"],"no-negated-in-lhs",["122"],["121"],["122"],"@typescript-eslint/no-unused-vars","'setUserId' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-neg